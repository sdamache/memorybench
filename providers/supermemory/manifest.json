{
	"manifest_version": "1",
	"provider": {
		"name": "supermemory",
		"type": "intelligent_memory",
		"version": "1.0.0"
	},
	"capabilities": {
		"core_operations": {
			"add_memory": true,
			"retrieve_memory": true,
			"delete_memory": true
		},
		"optional_operations": {
			"update_memory": true,
			"list_memories": true,
			"reset_scope": false,
			"get_capabilities": true
		},
		"system_flags": {
			"async_indexing": true
		},
		"intelligence_flags": {
			"auto_extraction": true,
			"graph_support": false
		}
	},
	"semantic_properties": {
		"update_strategy": "eventual",
		"delete_strategy": "immediate"
	},
	"conformance_tests": {
		"expected_behavior": {
			"convergence_wait_ms": 10000
		}
	}
}
